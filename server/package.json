{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e ./env/.env.development nodemon  src/index.ts ",
    "start": "dotenv -e ./env/.env.production nodemon src/index.ts",
    "test": "dotenv -e ./env/.env.development jest --config ./jest.config.js",
    "test:production": "dotenv -e ./env/.env.production jest --config ./jest.config.js",
    "prisma:migrate:dev": "dotenv -e ./env/.env.development prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e ./env/.env.production prisma migrate dev",
    "prisma:studio:prod": "dotenv -e ./env/.env.production prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "prisma": "^5.16.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-standard": "^12.0.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "bcrypt-ts": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/ts-standard/eslintrc.json"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "env": {
      "jest": true
    }
  }
}
